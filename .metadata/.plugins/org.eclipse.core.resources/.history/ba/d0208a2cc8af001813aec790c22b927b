import java.io.Serializable;
import java.util.ArrayList;

public class Family {
	private String childName;
	private String parentName;
	private Gender gender;
	private boolean isMale;

	
//public Family() {
//		this.childName = childName;
//		this.parentName = parentName;
//	}

public String getChildName() {
	return childName;
}
@Override
public String toString() {
	return "Family [childName=" + childName + ", parentName=" + parentName + ", gender=" + gender + ", isMale=" + isMale
			+ "]";
}
public boolean setChildName(String childName) {
	this.childName = childName;
	return isMale;
}

public String getParentName() {
	return parentName;
}

public boolean setParentName(String childName, String parentName, boolean isMale) {
	this.parentName = parentName;
	return isMale;
}

public Gender getGender() {
	return gender;
}

public void setGender(Gender gender) {
	this.gender = gender;
}


}