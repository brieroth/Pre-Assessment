import org.junit.Test;
import org.junit.Assert;


public class FamilyTDD {
	Family person = new Family("Ge", "lin", false);
	@Test
	public void testFamily() {
		
		//person.setParentName("Vera", "George", true);
		Assert.assertEquals("lin", person.getParentName(false));
		Assert.assertEquals("Ge",person.getChildName(false));
		
	}

}
